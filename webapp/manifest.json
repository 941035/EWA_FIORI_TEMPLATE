{
  "_version": "1.48.0",
  "sap.app": {
    "id": "com.parker.ewaformtemp",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.8.3",
      "toolsId": "4ea51277-b260-4493-9242-8a16b773b74c"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZEWA_GENRAL_INFO_PROJ_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "viewModel" :{
        "uri" :"model/viewData.json",
        "type" : "JSON"

      },
      "refreshDataModel" :{
        "uri" :"model/initialViewData.json",
        "type" : "JSON"

      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.112.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.parker.ewaformtemp.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      },
      "viewModel" :{
        "type" : "sap.ui.model.json.JSONModel",
        "dataSource": "viewModel"

      },
      "refreshDataModel":{
        "type" : "sap.ui.model.json.JSONModel",
        "dataSource": "refreshDataModel"

      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.parker.ewaformtemp.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteMainView",
          "pattern": "TargetMainView",
          "target": [
            "TargetMainView"
          ]
        },
        {
          "name": "TaskView",
          "pattern": "",
          "target": [
            "TaskView"
          ]
         
        },
        {
			   "pattern": "detail/{scenarioid}/{wfInstanceId}/{taskPath}",
			   "name": "wfobject",
			   "target": "object"
		    }
        
      ],
      "targets": {
        "TargetMainView": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "MainView",
          "viewName": "MainView"
        },
        "TaskView":{
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "EwaListView",
          "viewName": "EwaListView"
        },
        "object": {
          "viewName": "Approver",
          "viewId": "Approver",
          "viewLevel": 1,
          "transition": "slide",
          "clearControlAggregation": false
        }
      }
    },
    "rootView": {
      "viewName": "com.parker.ewaformtemp.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
